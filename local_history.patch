+  var outputDir = this.options.path || '.';
+  var outputFilename = path.join(outputDir, this.options.filename || DEFAULT_OUTPUT_FILENAME);
+  var publicPath = this.options.publicPath || compiler.options.output.publicPath;
+  if (publicPath) {
